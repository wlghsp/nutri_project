<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.bteam.api.mapper.Nutrient_Mapper">
	<insert id="insert">
		INSERT INTO NUTRIENT_BOARD
		VALUES (#{nutrient}, #{gender}, #{age}, #{Average_Intake}, #{Recommended_Intake}, 
			#{Sufficient_intake}, #{Maximum_Intake})
	</insert>

</mapper>